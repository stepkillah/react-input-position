{
  "name": "@stepkillah/react-input-position",
  "version": "1.4.0",
  "description": "A React component that decorates it's children with mouse/touch position tracking, a status toggle fired by click/gesture events, and more.",
  "keywords": [
    "touch",
    "tap",
    "gesture",
    "mouse",
    "click",
    "move",
    "cursor",
    "input",
    "position",
    "coordinates",
    "track",
    "event",
    "listener"
  ],
  "license": "MIT",
  "author": "Adam Risberg <myniztan@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/stepkillah/react-input-position.git"
  },
  "homepage": "https://adamrisberg.github.io/react-input-position",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "transpile": "rimraf dist && babel src -d dist --ignore src/__tests__/*",
    "prepublishOnly": "npm run transpile",
    "build": "rimraf examples/dist && webpack --mode production",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "npm run build && npm run deploy"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@types/jest": "^29.5.14",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "enzyme": "^3.11.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint-webpack-plugin": "^4.2.0",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1",
    "typescript-eslint": "^8.18.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
